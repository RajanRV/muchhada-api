{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "name": "Address"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "mobile": {
      "type": "string"
    },
    "pincode": {
      "type": "biginteger"
    },
    "address1": {
      "type": "string"
    },
    "address2": {
      "type": "string"
    },
    "landmark": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Home",
        "Work"
      ]
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "addresses"
    }
  }
}
